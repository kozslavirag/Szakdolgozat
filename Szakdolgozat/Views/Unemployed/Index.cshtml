@model IEnumerable<Szakdolgozat.Models.UnemployedModel>

@{
    ViewData["Title"] = "";
}

<h1>A 15–74 éves munkanélküliek száma</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumberofUnemployed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaleUnemployed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FemaleUnemployed)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumberofUnemployed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaleUnemployed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FemaleUnemployed)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Date">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Date">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Date">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

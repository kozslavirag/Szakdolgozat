@model IEnumerable<Szakdolgozat.Models.BusinessModel>

@{
    ViewData["Title"] = "";
}

<h1>A kiskereskedelmi eladási forgalom negyedévente (millió Ft)</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.OnlineBusiness)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Retail)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OnlineBusiness)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Retail)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Date">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Date">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Date">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
